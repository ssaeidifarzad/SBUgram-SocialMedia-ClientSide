<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="645.0" prefWidth="420.0" style="-fx-background-color: #1e1e32;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.OwnerProfileController">
    <children>
        <ImageView fx:id="profilePhoto" fitHeight="110.0" fitWidth="110.0" layoutX="33.0" layoutY="31.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../resources/user.png" />
            </image>
        </ImageView>
        <Text fx:id="ownerUsername" fill="WHITE" layoutX="182.0" layoutY="57.0" strokeType="OUTSIDE" strokeWidth="0.0" text="UserName">
            <font>
                <Font name="Century" size="20.0" />
            </font>
        </Text>
        <Text fx:id="ownerName" fill="WHITE" layoutX="182.0" layoutY="93.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name">
            <font>
                <Font name="Century" size="20.0" />
            </font>
        </Text>
        <Text fx:id="ownerBirthDate" fill="WHITE" layoutX="182.0" layoutY="129.0" strokeType="OUTSIDE" strokeWidth="0.0" text="BirthDate">
            <font>
                <Font name="Century" size="20.0" />
            </font>
        </Text>
      <Button fx:id="editProfileButton" layoutX="48.0" layoutY="165.0" mnemonicParsing="false" onAction="#loadEditProfilePage" style="-fx-background-color: #bbbbbb;" text="Edit Profile">
         <font>
            <Font name="Century" size="12.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
